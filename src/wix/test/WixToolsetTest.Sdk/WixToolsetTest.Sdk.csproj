<Project Sdk="MSTest.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <DefaultItemExcludes>TestData\**;$(DefaultItemExcludes)</DefaultItemExcludes>
    <RequiresNativeWixAssets>true</RequiresNativeWixAssets>
    <IsWixMSTestProject>true</IsWixMSTestProject>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="TestData\**" CopyToOutputDirectory="PreserveNewest" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\WixInternal.Core.MSTestPackage\WixInternal.Core.MSTestPackage.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="WixInternal.MSTestSupport" />
  </ItemGroup>
</Project>
